{
    "name": "Cookies Dashboard",
    "version": "1.0.0",
    "description": " This is an extension for cookies",
    "manifest_version": 3,
    "permissions": ["cookies", "activeTab"],
    "author": "Daniela Sanchez",
    "action":{
        "default_popup": "index.html",
        "default_title": "Cookies Dashboard"
    },
    "background":{
        "service_worker": "background.js"
    },
    "content_scripts":[
        {
            "matches":["<all_urls"],
            "js":["content.js"]
        }
    ]
}